<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
        package="com.example.pratapgurung.testnavigation">

    <application
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity android:name=".SignUp"></activity>
        <activity android:name=".loginActivity"></activity>
        <activity
                android:name=".MainActivity"
                android:label="@string/app_name"
                android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
                android:name=".profile"
                android:label="Profile"
                android:parentActivityName=".MainActivity">
            <meta-data
                    android:name="android.support.PARENT_ACTIVITY"
                    android:value="com.example.pratapgurung.testnavigation.MainActivity" />
        </activity>
        <activity
                android:name=".firstName"
                android:label="First Name"
                android:parentActivityName=".profile">
            <meta-data
                    android:name="android.support.PARENT_ACTIVITY"
                    android:value="com.example.pratapgurung.testnavigation.profile" />
        </activity>
        <activity
                android:name=".secondName"
                android:label="Second Name"
                android:parentActivityName=".profile">
            <meta-data
                    android:name="android.support.PARENT_ACTIVITY"
                    android:value="com.example.pratapgurung.testnavigation.profile" />
        </activity>
        <activity
                android:name=".password"
                android:label="Password"
                android:parentActivityName=".profile">
            <meta-data
                    android:name="android.support.PARENT_ACTIVITY"
                    android:value="com.example.pratapgurung.testnavigation.profile" />
        </activity>
        <activity
                android:name=".phoneNumber"
                android:label="Phone Number"
                android:parentActivityName=".profile">
            <meta-data
                    android:name="android.support.PARENT_ACTIVITY"
                    android:value="com.example.pratapgurung.testnavigation.profile" />
        </activity>
        <activity
                android:name=".email"
                android:label="Email"
                android:parentActivityName=".profile">
            <meta-data
                    android:name="android.support.PARENT_ACTIVITY"
                    android:value="com.example.pratapgurung.testnavigation.profile" />
        </activity>
        <activity
                android:name=".PastActivity"
                android:label="Past Activity"
                android:parentActivityName=".MainActivity">
            <meta-data
                    android:name="android.support.MainActivity"
                    android:value="com.example.pratapgurung.testnavigation.MainActivity" />
        </activity>
        <activity
                android:name=".CheckAgents"
                android:label="Check Agents"
                android:parentActivityName=".MainActivity">
            <meta-data
                    android:name="android.support.MainActivity"
                    android:value="com.example.pratapgurung.testnavigation.MainActivity" />
        </activity>
        <activity
                android:name=".RequestStatus"
                android:label="Request Status"
                android:parentActivityName=".MainActivity">
            <meta-data
                    android:name="android.support.MainActivity"
                    android:value="com.example.pratapgurung.testnavigation.MainActivity" />
        </activity>
        <activity
                android:name=".Help"
                android:label="Help"
                android:parentActivityName=".MainActivity">
            <meta-data
                    android:name="android.support.MainActivity"
                    android:value="com.example.pratapgurung.testnavigation.MainActivity" />
        </activity>
        <activity
                android:name=".sucessActivity"
                android:label="Sucess"
                android:parentActivityName=".MainActivity">
            <meta-data
                    android:name="android.support.MainActivity"
                    android:value="com.example.pratapgurung.testnavigation.MainActivity" />
        </activity>
        <activity
                android:name=".detail_service_request"
                android:label="Service Request"
                android:parentActivityName=".RequestStatus">
            <meta-data
                    android:name="android.support.RequestStatus"
                    android:value="com.example.pratapgurung.testnavigation.RequestStatus" />
        </activity>
    </application>

</manifest>